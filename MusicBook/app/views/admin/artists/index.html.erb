<p id="notice"><%= notice %></p>

<h1>Artists</h1>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Dob</th>
      <th>Placeofbirth</th>
      <th>State</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @artists.each do |artist| %>
      <tr>
        <td><%= artist.name %></td>
        <td><%= artist.dob %></td>
        <td><%= artist.placeOfBirth %></td>
        <td><%= artist.state %></td>
        <td><%= artist.address %></td>
        <td><%= link_to 'Show', artist %></td>
        <td><%= link_to 'Destroy', artist, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Artist', admin_artists_new %>
