<p id="notice"><%= notice %></p>

<div class="panel panel-success">
  <div class="panel-heading"><h1>Artists</h1></div>
  <div class="panel-body">
<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Dob</th>
      <th>Placeofbirth</th>
      <th>State</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @artists.each do |artist| %>
      <tr>
        <td><%= artist.name %></td>
        <td><%= artist.dob %></td>
        <td><%= artist.placeOfBirth %></td>
        <td><%= artist.state %></td>
        <td><%= artist.address %></td>
        <td><%= link_to 'Show', artist_show_path(artist) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

</div>
</div>